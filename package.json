    "body": "^5.1.0",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "if-env": "^1.0.4",
    "mongoose": "^5.10.13",
    "parser": "^0.1.4"
  },
  "devDependencies": {
    "concurrently": "^5.3.0"
  }
}
